(use-package :ui)(defclass foo-dialog (3d-dialog) ())(defmethod view-draw-contents ((w foo-dialog))  (call-next-method)    (with-3d-colors (*white-color* $chiseling-gray)    (draw-line-3d (make-point 6 (- (view-size-v w) 30))                   (make-point (- (view-size-h w) 6) (- (view-size-v w) 30))                  :direction :up                  :width 5)    (draw-line-3d (make-point 6 (- (view-size-v w) 20))                   (make-point (- (view-size-h w) 6) (- (view-size-v w) 20))                  :direction :down                  :width 1)))(make-instance 'foo-dialog  :view-size #@(320 200)  :view-subviews (list                   (make-instance 'ui::twist-down-dialog-item                    :view-position #@(230 10))                  (make-instance '3d-dialog-item                    :view-position #@(10 10)                    :view-size #@(100 50)                    :frame-width 1)                  (make-instance '3d-editable-text-dialog-item                    :view-position #@(10 70)                    :view-size #@(100 50))                  (make-instance '3d-dialog-item                    :view-position #@(120 10)                    :view-size #@(100 50)                    :bg-color 	(darken *red-color*)                    :shadow-color	(darken (darken *red-color*))                    :hilit-color	*red-color*                    :frame-width 1                    :inset #@(1 1)                    :outer-frame t)                  (make-instance '3d-dialog-item                    :view-position #@(120 66)                    :view-size #@(100 50)                    :bg-color 	(darken *green-color*)                    :shadow-color	(darken (darken *green-color*))                    :hilit-color	*green-color*                    :frame-width 1                    :inset #@(1 1)                    :outer-frame t)                  (make-instance '3d-dialog-item                    :view-position #@(230 66)                    :view-size #@(100 50)                    :bg-color 	(darken *light-blue-color*)                    :shadow-color	(darken (darken *light-blue-color*))                    :hilit-color	*light-blue-color*                    :frame-width 1                    :inset #@(1 1)                    :outer-frame t)                  (make-instance 'window-resize-dialog-item                    :cicn-transform #$ttLabel1                    :view-position #@(306 186))))(let ((w (make-instance '3d-dialog           :bg-color 10880517 :shadow-color 8193284 :hilit-color 14485510)))    (add-subviews w                (make-instance '3d-dialog-item                  :bg-color 10880517 :shadow-color 8193284 :hilit-color 14485510                  :shadow-position :topleft                  :view-position #@(2 2)                  :view-size (make-point (- (view-size-h w) 3) 24))                (make-instance '3d-dialog-item                  :bg-color 10880517 :shadow-color 8193284 :hilit-color 14485510                  :shadow-position :topleft                  :view-position #@(2 28)                  :view-size (make-point 40 (- (view-size-v w) 28 2)))                (make-instance '3d-button-dialog-item                  ; color -> fill shadow hilit                  :colors (list 10880517 8193284 14485510)                  :hilited-colors (list (darken 10880517) (darken 8193284) (darken 14485510))                  :view-position #@(48 32)                  :view-size #@(80 16)                  :view-font '("Geneva" :plain 9)                  :dialog-item-text "Button"                  :frame nil                  :square t                  :embossed t)))(let ((w (make-instance '3d-dialog           :bg-color 1608207 :shadow-color 1206284 :hilit-color 2078484)))    (add-subviews w                (make-instance '3d-dialog-item                  :bg-color 1608207 :shadow-color 1206284 :hilit-color 2078484                  :shadow-position :topleft                  :view-position #@(2 2)                  :view-size (make-point (- (view-size-h w) 3) 24))                (make-instance '3d-dialog-item                  :bg-color 1608207 :shadow-color 1206284 :hilit-color 2078484                  :shadow-position :topleft                  :view-position #@(2 28)                  :view-size (make-point 40 (- (view-size-v w) 28 2)))                (make-instance '3d-button-dialog-item                  ; color -> fill shadow hilit                  :colors (list 1608207 1206284 2078484)                  :hilited-colors (list (darken 1608207)                                        (darken 1206284)                                        (darken 2078484))                  :view-position #@(48 32)                  :view-size #@(80 16)                  :view-font '("Geneva" :plain 9)                  :dialog-item-text "Button"                  :frame nil                  :square t                  :embossed t)))(let ((w (make-instance '3d-dialog           :bg-color 164272 :shadow-color 156036 :hilit-color 175082)))    (add-subviews w                (make-instance '3d-dialog-item                  :bg-color 164272 :shadow-color 156036 :hilit-color 175082                  :shadow-position :topleft                  :view-position #@(2 2)                  :view-size (make-point (- (view-size-h w) 3) 24))                (make-instance '3d-dialog-item                  :bg-color 164272 :shadow-color 156036 :hilit-color 175082                  :shadow-position :topleft                  :view-position #@(2 28)                  :view-size (make-point 40 (- (view-size-v w) 28 2)))                (make-instance '3d-button-dialog-item                  ; color -> fill shadow hilit                  :colors (list 164272 156036 175082)                  :hilited-colors (list (darken 164272) (darken 156036) (darken 175082))                  :view-position #@(48 32)                  :view-size #@(80 16)     